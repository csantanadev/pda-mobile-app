{
  "expo": {
    "name": "Pelada dos Amigos",
    "slug": "pdamobile",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "5.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.ticeos.peladadosamigos",
      "versionCode": 5,
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ticeos.peladadosamigos",
      "buildNumber": "1.0.0"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#FFF"
    },
    "notification": {
      "icon": "./assets/icon_push.png"
    }
  }
}